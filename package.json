{
  "name": "skm-custom-log",
  "displayName": "Custom Log Highlighter",
  "description": "Custom syntax highlighting for .log files in VS Code",
  "version": "0.0.2",
  "publisher": "skm",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "custom-log",
      "aliases": ["Custom Log", "custom-log"],
      "extensions": [".log"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "custom-log",
      "scopeName": "source.custom-log",
      "path": "./syntaxes/custom-log.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/srijidhkm/skm-custom-log.git"
  }
}
