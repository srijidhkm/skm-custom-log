{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Custom Log",
	"patterns": [
		{
		"include": "#log-level"
		}
	],
	"repository": {
		"log-level": {
		"name": "keyword.other.log-level.custom-log",
		"match": "\\b(TRACE|DEBUG|INFO|WARN|WARNING|ERROR|FATAL)\\b",
		"captures": {
			"1": {
			"patterns": [
				{
				"match": "ERROR|FATAL|error|fatal|Error|Fatal",
				"name": "keyword.other.error.custom-log"
				},
				{
				"match": "INFO|Info",
				"name": "keyword.other.info.custom-log"
				},
				{
				"match": "DEBUG|TRACE|Debug|Trace",
				"name": "keyword.other.debug.custom-log"
				},
				{
				"match": "WARN|WARNING",
				"name": "keyword.other.warning.custom-log"
				}
			]
			}
		}
		}
	},
	"scopeName": "source.custom-log"
}